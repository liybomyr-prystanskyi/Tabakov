{{#header}}
<header class="header">
  <div class="container">
    <div class="row justify-content-center" style="background-color: #fff;">
      <div class="col-12 col-lg-10">
        <div class="row align-items-center">
          <div class="col-9 col-sm-4 col-md-3 col-lg-2">
            <a href="#" calss="logo">
              <img src="assets/img/logo.png" alt="logo" class="logo-image">
            </a>
          </div>
          <div class="col-3 col-sm-8 col-md-9 col-lg-10 position-static">
            <nav class="header__main-nav">
              <ul class="d-flex flex-column flex-lg-row justify-content-start justify-content-lg-end">
                {{#nav-items}}
                <li>
                  <a href="#" {{#current}} class="current" {{/current}}>
                    {{nav-item-text}}
                  </a>
                </li>
                {{/nav-items}}
              </ul>
              <div class="social-items-menu">
                <a href="#" class="fb"></a>
                <a href="#" class="instagram"></a>
              </div>
            </nav>
            <a href="#" class="burger-button">
              <span></span>
            </a>
          </div>
        </div>
      </div>
    </div>
  </div>
</header>
{{/header}}